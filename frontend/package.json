{
  "name": "frontend",
  "main": "index.js",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "npx @expo/cli start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "npx @expo/cli start --android",
    "ios": "npx @expo/cli start --ios",
    "web": "npx @expo/cli start --web",
    "web:clear": "npx @expo/cli start --web --clear",
    "web:open": "powershell -NoProfile -ExecutionPolicy Bypass -File ./start_expo.ps1",
    "lint": "expo lint",
    "test": "jest",
    "postinstall": "patch-package",
    "build:web": "expo export:web",
    "typecheck": "tsc --noEmit",
    "ci": "npm run lint && npm run typecheck && npm run test",
    "preflight": "npm run lint && npm run typecheck && npm run test && npm run build:web",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "^8.5.1",
    "@react-native-community/netinfo": "11.4.1",
    "@sentry/react-native": "7.8.0",
    "@shopify/flash-list": "^2.2.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "ajv": "^8.17.1",
    "axios": "^1.13.2",
    "expo": "~54.0.30",
    "expo-barcode-scanner": "~13.0.1",
    "expo-blur": "~15.0.8",
    "expo-brightness": "~14.0.8",
    "expo-camera": "~17.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.12",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-keep-awake": "~15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-notifications": "~0.32.15",
    "expo-router": "~6.0.21",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "fuse.js": "^7.1.0",
    "lottie-react-native": "^7.3.4",
    "metro": "^0.83.3",
    "metro-resolver": "^0.83.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.69.0",
    "react-native": "0.83.1",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-mmkv": "^4.1.0",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "^4.19.0",
    "react-native-svg": "^15.15.1",
    "react-native-web": "^0.21.2",
    "react-native-webview": "^13.16.0",
    "react-native-worklets": "^0.7.1",
    "use-debounce": "^10.0.6",
    "use-sync-external-store": "^1.6.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-react": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@expo/cli": "latest",
    "@expo/metro-runtime": "~6.1.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^10.1.10",
    "@storybook/addon-onboarding": "^10.1.10",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^10.1.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.7",
    "babel-plugin-polyfill-corejs3": "^0.13.0",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "eslint": "^9.39.2",
    "eslint-config-expo": "~10.0.0",
    "expo-doctor": "^1.17.14",
    "expo-module-scripts": "^5.0.8",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.16",
    "metro-react-native-babel-transformer": "^0.77.0",
    "patch-package": "^8.0.1",
    "react-test-renderer": "^19.2.3",
    "storybook": "^10.1.10",
    "typescript": "~5.9.3"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/app/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native|@react-native-community|expo(nent)?|@expo(nent)?/.*|@expo/.*|@unimodules/.*|react-native-svg|react-native-reanimated|@react-navigation/.*))"
    ]
  },
  "private": true
}
